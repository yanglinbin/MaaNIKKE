{
    "name": "MaaNIKKE",
    "url": "https://github.com/yanglinbin/MaaNIKKE",
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": "UnityWndClass*",
                "window_regex": "NIKKE",
                "input": 1,
                "screencap": 2
            }
        }
    ],
    "resource": [
        {
            "name": "国际服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "-m",
            "agent.main"
        ]
    },
    "task": [
        {
            "name": "方舟任务",
            "entry": "方舟",
            "option": [
                "拦截战",
                "异常个体拦截战boss选择",
                "模拟室",
                "模拟室_快速模拟",
                "无限之塔",
                "竞技场",
                "竞技场_新人竞技场",
                "竞技场_特殊竞技场",
                "竞技场_特殊竞技场_累计奖励领取"
            ]
        },
        {
            "name": "妮姬咨询",
            "entry": "妮姬每日咨询"
        },
        {
            "name": "百货商店",
            "entry": "百货商店",
            "option": [
                "普通商店",
                "竞技场商店",
                "竞技场商店_代码手册",
                "竞技场商店_代码手册宝箱",
                "竞技场商店_公司武器熔炉"
            ]
        },
        {
            "name": "付费商店",
            "entry": "付费商店"
        },
        {
            "name": "奖励领取",
            "entry": "奖励领取",
            "option": [
                "友情点赠送",
                "邮件领取",
                "任务领取",
                "通行证领取"
            ]
        },
        {
            "name": "前哨基地",
            "entry": "前哨基地",
            "option": [
                "派遣公告栏",
                "循环室"
            ]
        }
    ],
    "option": {
        "派遣公告栏": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "派遣公告栏": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "循环室": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
