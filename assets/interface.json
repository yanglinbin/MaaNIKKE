{
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": "UnityWndClass*",
                "window_regex": "NIKKE",
                "input": 2,
                "screencap": 2
            }
        }
    ],
    "resource": [
        {
            "name": "国际服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "普通商店",
            "entry": "普通商店",
            "option": [
                "百货商店",
                "竞技场商店",
                "代码手册",
                "代码手册宝箱",
                "公司武器熔炉"
            ]
        },
        {
            "name": "付费商店",
            "entry": "付费商店"
        }
    ],
    "option": {
        "百货商店": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "百货商店": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "竞技场商店": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "竞技场商店": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "代码手册": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "代码手册": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "代码手册宝箱": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "代码手册宝箱": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "公司武器熔炉": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "公司武器熔炉": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}